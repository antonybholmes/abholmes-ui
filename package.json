{
  "name": "@abholmes/ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,mjs,svelte,astro}\""
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      },
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      }
    },
    "./components": {
      "require": {
        "types": "./lib/cjs/components/index.d.ts",
        "default": "./lib/cjs/components/index.js"
      },
      "import": {
        "types": "./lib/esm/components/index.d.ts",
        "default": "./lib/esm/components/index.js"
      }
    },
    "./components/dropdown": {
      "require": {
        "types": "./lib/cjs/components/dropdown/index.d.ts",
        "default": "./lib/cjs/components/dropdown/index.js"
      },
      "import": {
        "types": "./lib/esm/components/dropdown/index.d.ts",
        "default": "./lib/esm/components/dropdown/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/lodash": "^4.14.197",
    "@types/uuid": "^9.0.2",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6"
  }
}
